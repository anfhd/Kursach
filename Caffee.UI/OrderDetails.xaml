<Window x:Class="Caffee.UI.OrderDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Caffee.UI"
        mc:Ignorable="d"
        Title="OrderDetails" Height="450" Width="800">
    <Grid>
        <Label Margin="0,78,690,270">ID</Label>
        <Label x:Name="IDLabel" Margin="226,304,472,44">id</Label>

        <Label Margin="279,199,431,149">Table No.</Label>
        <Label x:Name="TableNumberLabel" Margin="668,134,42,211">no</Label>

        <Label Margin="444,242,266,90">Seats</Label>
