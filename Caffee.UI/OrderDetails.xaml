        <Label x:Name="StatusLabel" Margin="184,218,526,114">stat</Label>

        <Label Margin="73,174,621,173">Created at</Label>
        <Label x:Name="CreationTimeLabel" Margin="604,43,106,305">crat</Label>

        <Label Margin="534,173,190,186">Waiter</Label>
        <Label x:Name="WaiterLabel" Margin="470,78,201,254">waiter</Label>

        <Label Margin="358,140,313,192">Discount</Label>
        <Label x:Name="DiscountLabel" Margin="328,46,382,276">disc</Label>

