<Window x:Class="Caffee.UI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Caffee.UI"
        mc:Ignorable="d"
        Title="Login" Height="206" Width="175">
    <Grid>
        <Label Margin="0,0,0,70">Login:</Label>
        <TextBox x:Name="LoginTextBox" Margin="10,27,10,109"></TextBox>

        <Label Margin="0,55,0,15">Password:</Label>
        <TextBox x:Name="PasswordTextBox" Margin="10,81,10,52"></TextBox>

        <Button x:Name="LogInButton" Margin="0,127,0,0" Click="LogInButton_Click">Log In</Button>

    </Grid>
</Window>
